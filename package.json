{
  "name": "vue-map-drawing",
  "version": "0.1.1",
  "description": "A Vue 3 library for drawing shapes on Google Maps with edge snapping support",
  "type": "module",
  "main": "./dist/vue-map-drawing.umd.cjs",
  "module": "./dist/vue-map-drawing.js",
  "exports": {
    ".": {
      "import": "./dist/vue-map-drawing.js",
      "require": "./dist/vue-map-drawing.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "google-maps",
    "drawing",
    "polygon",
    "circle",
    "rectangle",
    "polyline",
    "snapping",
    "map",
    "shape-editor"
  ],
  "author": "Fatih Aydin",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.0.0"
  },
  "homepage": "https://github.com/aydinfatih/vue-map-drawing#readme",
  "bugs": {
    "url": "https://github.com/aydinfatih/vue-map-drawing/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aydinfatih/vue-map-drawing.git"
  }
}
